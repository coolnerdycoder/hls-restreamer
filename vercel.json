{
  "functions": {
    "api/playlist.js": {
      "maxDuration": 30,
      "memory": 1536
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS, HEAD"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Accept, Range"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/playlist/(.*)",
      "destination": "/api/playlist?id=$1"
    },
    {
      "source": "/channel/(.*)",
      "destination": "/api/playlist?id=$1"
    }
  ]
}